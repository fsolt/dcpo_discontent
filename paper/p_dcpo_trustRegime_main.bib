	keywords = {populism},
	number = {1},
	pages = {362-372},
	title = {State of the Field: How to Study Populism and Adjacent Topics? A Plea for Both More and Less Focus},
	volume = {58},
	year = {2019},
	bdsk-file-1 = {},
